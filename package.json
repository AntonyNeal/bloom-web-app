{
    "name":  "life-psychology-frontend",
    "private":  true,
    "version":  "0.0.0",
    "type":  "module",
    "scripts":  {
                    "dev":  "vite",
                    "build":  "node scripts/update-sw-version.cjs \u0026\u0026 node scripts/select-config.js \u0026\u0026 vite build",
                    "build:ci":  "export VITE_ENVIRONMENT=${VITE_ENVIRONMENT:-production} \u0026\u0026 node scripts/set-build-iteration.js \u0026\u0026 node scripts/update-sw-version.cjs \u0026\u0026 vite build",
                    "build:analyze":  "npm run build \u0026\u0026 npx vite-bundle-analyzer dist",
                    "build:env":  "node scripts/select-config.js",
                    "manage-swa":  "node scripts/manage-swa.js",
                    "lint":  "eslint src --max-warnings 0",
                    "lint:ci":  "npm run format \u0026\u0026 npm run lint \u0026\u0026 npm run format:check \u0026\u0026 npm run type-check",
                    "lint:fix":  "eslint src --fix",
                    "preview":  "vite preview",
                    "preview:ci":  "vite preview --port 5174 --host 0.0.0.0 --strictPort",
                    "format":  "prettier --write src/**/*.{ts,tsx,css,md}",
                    "format:check":  "prettier --check src/**/*.{ts,tsx,css,md}",
                    "type-check":  "tsc --noEmit",
                    "prepare":  "husky",
                    "test":  "playwright test",
                    "test:prestaging":  "playwright test -c playwright.prestaging.config.ts",
                    "test:visual":  "playwright test -c playwright.visual.config.ts",
                    "test:visual:ab":  "playwright test -c playwright.ab-visual.config.ts",
                    "test:visual:legacy":  "SKIP_LEGACY_VISUAL=false playwright test -c playwright.ab-visual.config.ts --grep=\u0027legacy-visual-regression\u0027",
                    "test:ui":  "playwright test --ui",
                    "test:headed":  "playwright test --headed",
                    "test:unit":  "vitest",
                    "test:unit:ui":  "vitest --ui",
                    "test:unit:run":  "vitest run",
                    "test:visual:update":  "UPDATE_BASELINES=true npm run test:visual:ab",
                    "test:visual:update:legacy":  "UPDATE_BASELINES=true npm run test:visual:legacy",
                    "update-version":  "node scripts/update-version.js",
                    "manage-baselines":  "node scripts/manage-baselines.js",
                    "generate-webp":  "node scripts/generate-webp.js",
                    "security:audit":  "npm audit --audit-level=moderate",
                    "deps:check":  "npm outdated",
                    "deps:update":  "npm update",
                    "clean":  "rm -rf dist node_modules/.vite node_modules/.tmp",
                    "dev:website":  "pnpm --filter website dev",
                    "dev:bloom":  "pnpm --filter bloom dev",
                    "build:website":  "pnpm --filter website build",
                    "build:bloom":  "pnpm --filter bloom build"
                },
    "lint-staged":  {
                        "src/**/*.{ts,tsx}":  [
                                                  "eslint --fix",
                                                  "prettier --write"
                                              ],
                        "src/**/*.{css,md}":  [
                                                  "prettier --write"
                                              ]
                    },
    "dependencies":  {
                         "@actions/core":  "^1.6.0",
                         "@actions/http-client":  "^2.2.3",
                         "@headlessui/react":  "^2.2.7",
                         "@heroicons/react":  "^2.2.0",
                         "@microsoft/applicationinsights-web":  "^3.3.9",
                         "@stripe/react-stripe-js":  "^5.4.0",
                         "@stripe/stripe-js":  "^8.5.2",
                         "@tailwindcss/forms":  "^0.5.10",
                         "@tanstack/react-query":  "^5.85.5",
                         "@testing-library/dom":  "^10.4.1",
                         "axios":  "^1.11.0",
                         "dotenv":  "^17.2.3",
                         "googleapis":  "^165.0.0",
                         "lucide-react":  "^0.542.0",
                         "mssql":  "^12.1.0",
                         "open":  "^10.2.0",
                         "openai":  "^5.18.1",
                         "react":  "^18.3.1",
                         "react-dom":  "^18.3.1",
                         "react-ga4":  "^2.1.0",
                         "react-helmet-async":  "^2.0.5",
                         "react-router-dom":  "^7.8.1",
                         "web-vitals":  "^5.1.0"
                     },
    "engines":  {
                    "node":  "\u003e=20.19.0"
                },
    "devDependencies":  {
                            "@babel/plugin-transform-react-jsx":  "^7.27.1",
                            "@eslint/js":  "^9.33.0",
                            "@playwright/test":  "^1.55.0",
                            "@testing-library/jest-dom":  "^6.8.0",
                            "@testing-library/react":  "^14.3.1",
                            "@testing-library/user-event":  "^14.6.1",
                            "@types/node":  "^24.3.0",
                            "@types/pngjs":  "^6.0.5",
                            "@types/react":  "^18.3.18",
                            "@types/react-dom":  "^18.3.5",
                            "@typescript-eslint/eslint-plugin":  "^8.40.0",
                            "@typescript-eslint/parser":  "^8.40.0",
                            "@vitejs/plugin-react":  "^5.0.0",
                            "autoprefixer":  "^10.4.21",
                            "eslint":  "^9.33.0",
                            "eslint-config-prettier":  "^10.1.8",
                            "eslint-plugin-prettier":  "^5.5.4",
                            "eslint-plugin-react-hooks":  "^5.2.0",
                            "eslint-plugin-react-refresh":  "^0.4.20",
                            "globals":  "^16.3.0",
                            "google-ads-api":  "^17.1.0",
                            "husky":  "^9.1.7",
                            "jsdom":  "^26.1.0",
                            "lint-staged":  "^16.1.5",
                            "pixelmatch":  "^5.3.0",
                            "pngjs":  "^7.0.0",
                            "postcss":  "^8.5.6",
                            "prettier":  "^3.6.2",
                            "sharp":  "^0.34.3",
                            "tailwindcss":  "^3.4.17",
                            "terser":  "^5.44.0",
                            "typescript":  "~5.8.3",
                            "typescript-eslint":  "^8.39.1",
                            "vite":  "^7.1.2",
                            "vite-plugin-remove-console":  "^2.2.0",
                            "vitest":  "^3.2.4"
                        }
}
